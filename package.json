{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.14",
    "@formily/antd": "^1.3.8",
    "@formily/antd-components": "^1.3.8",
    "@types/echarts": "^4.9.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.27",
    "antd": "^4.9.2",
    "echarts": "^4.9.0",
    "highlight.js": "^10.3.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "redux": "^4.0.5",
    "umi": "^3.2.27",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/preset-ui": "^2.2.8"
  }
}
